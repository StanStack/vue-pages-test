      }
    }],
    "stage-2"
  ],
  "ignore": [
    "./src/assets/libs/mui.min.js",
    "./src/assets/libs/mui.picker.min.js",
    "./src/assets/libs/mui.poppicker.min.js"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
